{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "groups": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::group.group",
      "mappedBy": "teacher"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "name": {
      "type": "string"
    },
    "family": {
      "type": "string"
    },
    "students": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::student.student",
      "mappedBy": "users_permissions_user"
    },
    "pinterestAccessToken": {
      "type": "text"
    },
    "pinterestRefreshToken": {
      "type": "text"
    },
    "guides": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::guide.guide",
      "mappedBy": "users_permissions_user"
    },
    "creations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::creation.creation",
      "mappedBy": "users_permissions_user"
    },
    "savedGuides": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::guide.guide",
      "mappedBy": "savedBy"
    },
    "portfolio": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::portfolio.portfolio",
      "mappedBy": "users_permissions_user"
    },
    "skill_trees": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::skill-tree.skill-tree",
      "mappedBy": "author"
    },
    "savedSkillTrees": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill-tree.skill-tree",
      "mappedBy": "savedBy"
    }
  }
}
